{% extends 'layouts/sidebar' %}
{% block content/sidecontent %}
<div class='bg-light p-4 mb-5 mt-5 mt-sm-0'>
  <h5 class='mb-4 text-primary'>{% editable_text "Sidebar subheader", format: raw, rows: 2 %}Question still not answered?{% endeditable_text %}</h5>
  <p class='mb-0'>
    {% editable_text "Sidebar blurb", format: raw, rows: 4 %}
    Please refer to the
    <a href='/portfolio/selection-criteria' style='text-decoration:underline;'>tenant selection criteria</a>
    and the
    <a href='/purpose/faq' style='text-decoration:underline;'>frequently asked questions</a>
    before contacting CIRE. If you still have questions please
    <a href='/contact-us' style='text-decoration:underline;'>contact us.</a>
    {% endeditable_text %}
  </p>
</div>
{% endblock %}
{% block content/main %}
<div class='accordion' id='faq_accordion'>
  {% for question in contents.questions %}
  <div class='card border-0'>
    <div class='card-header border-bottom py-3 bg-light' id='headingOne_heading-{{ question._slug }}'>
      <p class='mb-0'>
        <a aria-controls='collapseOne' aria-expanded='true' class='collapsed mb-0' data-target='#collapse-{{ question._slug }}' data-toggle='collapse' href='#'>{{ question.title }}</a>
      </p>
    </div>
    <div aria-labelledby='heading-{{ question._slug }}' class='collapse' data-parent='#faq_accordion' id='collapse-{{ question._slug }}'>
      <div class='card-body p-3'>
        <p class='mb-0'>{{ question.answer }}</p>
      </div>
    </div>
  </div>
  {% endfor %}
</div>
{% endblock %}